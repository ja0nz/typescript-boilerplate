{
  "name": "typescript-boilerplate-2021",
  "version": "0.4.2",
  "description": "Boilerplate for TypeScript projects (Node.js and browser modules)",
  "author": "Ja0nz <mail@ja.nz>",
  "repository": "https://github.com/ja0nz/typescript-boilerplate",
  "license": "MIT",
  "keywords": [
    "typescript",
    "boilerplate",
    "snowpack"
  ],
  "types": "./dist/tsc/main.d.ts",
  "browser": "./dist/esbuild/browser.js",
  "scripts": {
    "eval": "ts-node src/eval.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "uvu -r ts-node/register",
    "clean": "del build",
    "repl": "ts-node",
    "gendocs": "typedoc --entryPoints src/main.ts",
    "build": "$npm_execpath run clean && snowpack build",
    "start": "snowpack dev"
  },
  "devDependencies": {
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.22.0 ",
    "snowpack": "^3.4.0",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.3",
    "uvu": "^0.5.1"
  }
}
